"2	LINE	0
	# Grafana configuration
	grafana_security:
	  admin_user: admin
	  admin_password: password
	
	grafana_datasources:
	  - name: "Prometheus"
	    type: "prometheus"
	    url: "http://{{ groups['monitoring-stack'][0] }}:9090"
	
	# Import Grafana Public Dashboards
	grafana_dashboards:
	  - dashboard_id: '7587'
	    revision_id: '3'
	    datasource: 'Prometheus'
	
	# Update Prometheus Targets Here
	prometheus_targets:
	  node:
	    - targets: "{{ groups['node'] | map('regex_replace', '^(.*)$', '\\1:9100') | list }}"
	  process_exporter:
	    - targets: "{{ groups['node'] | map('regex_replace', '^(.*)$', '\\1:9256') | list }}"
	  alertmanager:
	    - targets: "{{ groups['monitoring-stack'] | map('regex_replace', '^(.*)$', '\\1:9093') | list }}"
	
	# Define environment and instance variables
	prometheus_environment: 'prod'  # Modify as needed
	prometheus_instance: '{{ ansible_fqdn | default(ansible_host) }}'
	
	# Prometheus configuration
	prometheus_scrape_configs:
	  - job_name: "prometheus"
	    metrics_path: "{{ prometheus_metrics_path }}"
	    static_configs:
	      - targets:
	          - "{{ ansible_fqdn | default(ansible_host) | default('localhost') }}:9090"
	        labels:
	          environment: '{{ prometheus_environment }}'
	          instance: '{{ prometheus_instance }}'
	
	  - job_name: "node"
	    file_sd_configs:
	      - files:
	          - "{{ prometheus_config_dir }}/file_sd/node.yml"
	
	  - job_name: "process_exporter"
	    file_sd_configs:
	      - files:
	          - "{{ prometheus_config_dir }}/file_sd/process_exporter.yml"
	
